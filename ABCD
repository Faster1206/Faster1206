public class student1
{
int id;
String name;
student1()
{
System.out.println("this aa defualt constructor ");
}
student1(int i,String n){
id=i;
name=n;
}    
void teacher()
{
System.out.println("this method overloading ");
}
void teacher(int a,int b)
{
int i,k;
i=a;
k=b;
System.out.println(i+k);
}
public static void main(String[] args)
{
student1 s = new student1();
System.out.println("\n Default Constructor values:\n");
System.out.println("Student id:"+s.id+"\nstudent name:\n"+s.name);
System.out.println("\nparameterized Constructor values: \n");
student1 s1 = new student1(10,"david");
student1 t=new student1();
t.teacher();
t.teacher(2,3);
}
}
 
